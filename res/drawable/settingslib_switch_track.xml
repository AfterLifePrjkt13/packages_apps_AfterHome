<?xml version="1.0" encoding="utf-8"?>
<inset
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="8dp"
    android:insetRight="8dp">
    <selector>
        <item
            android:state_checked="true"
            android:drawable="@drawable/settingslib_track_on_background"/>
        <item
            android:state_checked="false"
            android:drawable="@drawable/settingslib_track_off_background"/>
        <item
            android:state_enabled="false"
            android:drawable="@drawable/settingslib_track_disabled_background"/>
    </selector>
</inset>